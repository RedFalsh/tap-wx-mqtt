
.imgImage {
  width: 64px;
  height: 64px;
}

.box {
  height: 200px;
  width: 100%;
  background: white;
  border: 2px solid #fff;
  border-radius: 30px;
  margin-top: 20px;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
}

.box_2 {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
}

.status {
  height: 20%;
  width: 100%;
  color: #ece2e2;
  text-align: center;
  align-items: center;
}

.btn-row {
  height: 30%;
  width: 100%;
  align-items: center;
  display: flex;
  direction: row;
  justify-content: space-around;
}

.btn {
  width: 106rpx;
  height: 106rpx;
  border: solid 1px #D0D0D0;
  border-radius: 53rpx;
  align-items: center;
  display: flex;
  font-size: 32px;
  color: #D0D0D0;
  justify-content: center;
}

.btn-press {
  background: #D0D0D0;
  opacity: 0.7;
  transform: scale(1.1,1.1);
}

.btn-img {
  width: 64rpx;
  height: 64rpx;
}
